<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WYLButton">
        <attr name="background_color" format="color" />
        <attr name="press_background_color" format="color" />
        <attr name="background_image" format="reference"/>
        <attr name="press_background_image" format="reference"/>
        <attr name="text_color" format="color"/>
        <attr name="press_text_color" format="color"/>
        <attr name="radius" format="dimension"/>
        <attr name="radius_lt" format="dimension"/>
        <attr name="radius_rt" format="dimension"/>
        <attr name="radius_rb" format="dimension"/>
        <attr name="radius_lb" format="dimension"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="stroke_color" format="color"/>
        <attr name="keep_color" format="boolean"/>
        <attr name="padding_left" format="dimension"/>
        <attr name="padding_right" format="dimension"/>
        <attr name="padding_top" format="dimension"/>
        <attr name="padding_bottom" format="dimension"/>
        <attr name="press_stroke_color" format="color"/>
        <attr name="dashGap" format="dimension"/>
        <attr name="dashWidth" format="dimension"/>
        <attr name="gradientOrientation">
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
        <attr name="centerColor" format="color" />
        <attr name="press_startColor" format="color" />
        <attr name="press_endColor" format="color" />
        <attr name="press_centerColor" format="color" />
        <attr name="shapeType">
            <enum name="LINEAR_GRADIENT" value="0" />
            <enum name="OVAL" value="1" />
            <enum name="LINE" value="2" />
        </attr>
        <attr name="gradientType">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>
    </declare-styleable>
</resources>